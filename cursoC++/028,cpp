#include <iostream>
#include <stack>

using namespace std;

int main(){


    stack <string> cartas;

    cartas.push("Rei de Copas");
    cartas.push("Rei de Espadas");
    cartas.push("Rei de Ouros");
    cartas.push("Rei de Paus");

    cout << "Tamanho da Pilha: " << cartas.size();



    return 0;
}